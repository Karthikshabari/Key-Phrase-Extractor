{
  "expo": {
    "name": "Keyphrase Extractor",
    "slug": "keyphrase-extractor",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#6A5ACD"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.keyphraseextractor.app"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#6A5ACD"
      },
      "package": "com.keyphraseextractor.app",
      "permissions": ["android.permission.DETECT_SCREEN_CAPTURE"]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "keyphrase-extractor"
      }
    },
    "plugins": [],
    "updates": {
      "fallbackToCacheTimeout": 0
    }
  }
}
